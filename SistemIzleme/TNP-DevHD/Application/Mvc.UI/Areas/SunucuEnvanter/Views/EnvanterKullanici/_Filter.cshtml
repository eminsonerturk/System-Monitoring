@model  Application.ViewModels.Envanter.KullaniciSearchViewModel
@using Rota2.ResourceProviders
<div class="form-group">
    @Html.Label(RResx.GetResource("Envanter", "LabelKullaniciAd"), new { @class = "col-lg-1 control-label" })
    <div class="col-lg-3">
        @Html.RFilterPageEditorFor(p => p.KullaniciAd)
    </div>
    <label class = "col-lg-1 control-label" >E-Mail</label>
    <div class="col-lg-3">
        @Html.RFilterPageEditorFor(p => p.KullaniciEMail)
    </div>
    @Html.Label(RResx.GetResource("Envanter", "LabelIsAdmin"), new { @class = "col-lg-1 control-label" })
    <div class="col-lg-3">
        @Html.RFilterPageEditorFor(p => p.IsAdmin)
    </div>
</div>



