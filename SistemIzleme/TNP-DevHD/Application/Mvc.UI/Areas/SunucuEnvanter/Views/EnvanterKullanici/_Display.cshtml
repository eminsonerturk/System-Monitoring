@model Application.ViewModels.Envanter.KullaniciCrudViewModel

<h2>Kullanıcı Bilgileri</h2>
@Html.HiddenFor(p => p.Id)
<div class="form-group">
    @Html.LabelFor(p => p.KullaniciAd, new { @class = "col-lg-2 control-label" })
    <div class="col-lg-3">
        @Html.TextBoxFor(p => p.KullaniciAd, "", new { @class = "form-control", disabled = "disabled" })
    </div>
</div>
<div class="form-group">
    @Html.LabelFor(p => p.KullaniciEMail, new { @class = "col-lg-2 control-label" })
    <div class="col-lg-3">
        @Html.TextBoxFor(p => p.KullaniciEMail, "", new { @class = "form-control", disabled = "disabled" })
    </div>
</div>
<div class="form-group">
    @Html.LabelFor(p => p.IsAdmin, new { @class = "col-lg-2 control-label" })
    <div class="col-lg-3">
        @Html.TextBoxFor(p => p.IsAdmin, "", new { @class = "form-control", disabled = "disabled" })
    </div>
</div>
